#include <iostream>
#include<cmath>
using namespace std;

int main() {
	// your code goes here
	int t;cin>>t;while(t--){
	    int l,v1,v2;
	    cin>>l>>v1>>v2;
	    if(l%v1!=0){
	        v1=(l/v1)+1;
	    }
	    else{
	        v1=l/v1;
	    }
	    if(l%v2!=0){
	        v2=(l/v2)+1;
	    }
	    else{
	        v2=l/v2;
	    }
	   l=v1-v2-1;
	   cout<<l<<endl;
	   
	}
	return 0;
}
