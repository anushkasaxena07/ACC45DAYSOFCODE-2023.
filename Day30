#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int a,b,c,d;
	    cin>>a>>b>>c>>d;
	    int s=a-b;
	   int  j=c-d;
	   int t=s+j;
	   //cout<< t;
	   if(t<0)
	   {
	       cout<<"yes"<<endl;
	   }
	   else
	   {
	       cout<<"no"<<endl;
	   }
	}
	return 0;
}
